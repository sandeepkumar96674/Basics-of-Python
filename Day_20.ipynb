   "source": [
    "def dates(year,month):\n",
    "    if year%4==0 and year%100 !=0:\n",
    "        return \"Leap Year\"\n",
    "    elif year%100==0 and year%400==0:\n",
    "        return \"Leap Year\"\n",
    "    else:\n",
    "        return \"No leap Year\"\n",
    "year=int(input(\"Enter the year:\"))\n",
    "month=int(input(\"Enter the Month:\"))\n",
    "dates(year,month)"
   ]
  }
